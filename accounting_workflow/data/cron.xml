<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_check_quotations_exipry_date" model="ir.cron">
        <field name="name">Check Quotations Expiry Date</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="time.strftime('%Y-%m-%d 22:00:00')" />
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_sale_order"/>
        <field name="code">model._cron_check_quotation_expiry_days()</field>
        <field name="state">code</field>
    </record>

